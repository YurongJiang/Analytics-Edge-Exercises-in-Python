<span class="nb">print</span><span class="p">(</span><span class="s1">&#39;</span><span class="se">\t</span><span class="s1"> Center 2:&#39;</span><span class="p">,</span><span class="n">dem2</span><span class="o">.</span><span class="n">pi</span><span class="p">,</span><span class="s1">&#39;</span><span class="se">\t</span><span class="s1">Valid RHS:&#39;</span><span class="p">,</span><span class="n">dem2</span><span class="o">.</span><span class="n">sarhslow</span><span class="p">,</span><span class="s1">&#39;to&#39;</span><span class="p">,</span><span class="n">dem2</span><span class="o">.</span><span class="n">sarhsup</span><span class="p">)</span>
